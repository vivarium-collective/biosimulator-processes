<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model id="mTOR_Signalling_Model" name="mTOR Signalling Network Model">
    <listOfCompartments>
      <compartment id="cytosol" name="Cytosol" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="mTORC1" compartment="cytosol" initialConcentration="250"/>
      <species id="pmTORC1" compartment="cytosol" initialConcentration="0"/>
      <species id="mTORC2" compartment="cytosol" initialConcentration="200"/>
      <species id="pmTORC2" compartment="cytosol" initialConcentration="0"/>
      <species id="DEPTOR" compartment="cytosol" initialConcentration="350"/>
      <species id="pDEPTOR" compartment="cytosol" initialConcentration="0"/>
      <species id="IR" compartment="cytosol" initialConcentration="50"/>
      <species id="pIR" compartment="cytosol" initialConcentration="0"/>
      <species id="IRS" compartment="cytosol" initialConcentration="100"/>
      <species id="pIRS" compartment="cytosol" initialConcentration="0"/>
      <species id="Akt" compartment="cytosol" initialConcentration="100"/>
      <species id="pAkt" compartment="cytosol" initialConcentration="0"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="Km1" value="95"/>
      <parameter id="V1" value="1"/>
      <!-- Additional parameters -->
    </listOfParameters>
    <listOfReactions>
      <reaction id="reaction1" reversible="false">
        <listOfReactants>
          <speciesReference species="IR"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="pIR"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> compartment </ci>
              <apply>
                <ci> Henri_Michaelis_Menten__irreversible </ci>
                <ci> IR </ci>
                <ci> Km1 </ci>
                <ci> V1 </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <!-- Additional reactions -->
    </listOfReactions>
  </model>
</sbml>